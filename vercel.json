{
    "version": 2,
    "builds": [
        {
            "src": "cf_bot.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/webhook",
            "dest": "cf_bot.py",
            "methods": ["POST"]
        },
        {
            "src": "/",
            "dest": "cf_bot.py",
            "methods": ["GET"]
        }
    ]
} 